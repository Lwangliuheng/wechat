/* pages/main/main.wxss */
.mainUI {
  width: 100%;
  height:100%;
  margin: 0;
  padding: 0;
  position:fixed;
  top: 0; 
  background-color:#fff;  
  z-index: 10; 
}
.tips{
  position: relative;
  margin-bottom: 20rpx;
  
}
.tips .mainLogo{
  width: 100%;

}
.claimBox{
  width: 90%;
  margin: 0 auto;
  margin-bottom: 15rpx;
}
.selfClaims{
  width: 100%;
}
.mainToptext{
  font-size: 22px;
  font-weight: 600;
  position: absolute;
  top:10vh;
  color: #fff;
  padding-left: 35rpx;
}
.mainHeadtext{
  position: absolute;
   top:16vh;
   padding-left: 45rpx;
   color: #fff;
   font-size: 16px;
   line-height:30rpx;
}
.mainTop{
  font-size: 18px;
  font-weight: 600;
  position: absolute;
  top:15rpx;
  color: #fff;
  padding-left: 35rpx;
}
.mainHead{
  position: absolute;
   top:90rpx;
   padding-left: 45rpx;
   color: #fff;
   font-size: 16px;
   line-height:30rpx;
}
.buttonText{
  border: 1px solid #fff;
  border-radius: 22rpx;
  color: #fff;
  position: absolute;
  top: 90rpx;
  padding: 16rpx 30rpx;
  text-align: center;
  margin-left: 35rpx;
}
.openClaim{
  top: 150rpx;
}
.inputBox{
  display:flex;
  margin: 10vw 20rpx;
}
.buttonBox{
  margin-top: 20vw;
}
.lableText{
  display:inline-block;
  line-height: 35rpx;
  padding-right:8rpx;
}
.inputtelBox{
  height:35rpx;
  border:1px solid #bbb;
  width: 60vw;
  text-align: left;
  text-indent: 3rpx;
  border-radius: 6rpx;
}
.loginIn{
  color:#fff;
  background: #35A0FF;
  width: 80vw;

}
.guide-box {
  text-align: center;
}
.guide {
  display: inline-block;
  width: 30vw;
  height: 30vw;
  background-color: rgba(0, 0, 0, 0.55);
  text-align: center;
  margin: 1.5px;
  vertical-align: top;
}
.guide-nomore {
  display: inline-block;
  width: 30vw;
  height: 30vw;
  text-align: center;
  margin: 1.5rpx;
  vertical-align: top;
}
.guide-nomore-bk {
  background-color: rgba(0, 0, 0, 0.3);
}
.guide_icon {
  display: block;
  width: 36rpx; 
  height: 36rpx; 
  margin:20rpx auto 0;
}
.guide_label {
  display: block;
  margin-top: 10rpx;
  color: #CFE4FF;
  font-size: 14px;
}
.guide_nomore_label {
  margin-top: 30rpx;
  color: rgba(256, 256, 256, 0.3);
  font-size: 14px;
}
.logo-box {
  position: absolute;
  width: 100vw;
  bottom: 36rpx;
  text-align: center;
}
.logo {
  width: 100vw;
  height: 100%;
}