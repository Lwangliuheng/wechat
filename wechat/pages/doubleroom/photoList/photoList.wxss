.listTitle{
    height: 108rpx;
    line-height: 108rpx;
    color:#222;
    font-size:40rpx;
    padding-left: 30rpx;
    font-weight: 700;
}
.photoBox{
    padding-left: 30rpx;
}
.photoItem{
    width:336rpx;
    float: left;
    margin-right: 18rpx;
}
.photoView{
    width:336rpx;
    height: 200rpx;
    border-radius:30rpx;
    overflow: hidden;
}
.photoImg{
    width: 336rpx;
    height: 200rpx;
}
.photoInfo{
    font-size:30rpx;
    color:#222;
    display: block;
    height: 84rpx;
    line-height: 84rpx;
    text-align: center;
}
.swipeWrap{
    position: fixed;
    left: 0;
    top: 0;
    z-index: 10;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,1);
}
.slide-image{
    width:750rpx;
    display:block;
    position: absolute;
    top: 50%;
    transform:translate3d(0,-50%,0);
}
swiper{
    width:100%;
    height: 100%;
    position: absolute;
    top: 50%;
    transform:translate3d(0,-50%,0);
}
.photoText{
    display: inline-block;
    width:100%;
    text-align: center;
    color: #fff;
    position: absolute;
    top: 26%;
    z-index: 30;
}
.numText{
    display: inline-block;
    width:80%;
    right: 0;
    text-align: right;
    padding-right: 33rpx;
    color: #fff;
    position: absolute;
    bottom: 26%;
    z-index: 30;
}